{
	"version": 2,
	"builds": [
	  {
		"src": "api/app.py",
		"use": "@vercel/python"
	  },
	  {
		"src": "package.json",
		"use": "@vercel/static-build",
		"config": { "distDir": ".svelte-kit/output" }
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "api/app.py"
	  },
	  {
		"src": "/(.*)",
		"dest": "/"
	  }
	]
  }
  